{
  "name": "isra-seeds-backend",
  "version": "1.0.0",
  "description": "Backend pour le système de traçabilité des semences ISRA",
  "main": "dist/server.js",
  "scripts": {
    "build": "tsc",
    "start": "node dist/server.js",
    "dev": "nodemon --exec ts-node --esm src/server.ts",
    "dev:alt": "nodemon --exec \"ts-node -r tsconfig-paths/register src/server.ts\"",
    "dev:simple": "ts-node src/server.ts",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "db:migrate": "prisma migrate dev",
    "db:generate": "prisma generate",
    "db:seed": "tsx --tsconfig prisma/tsconfig.json prisma/seed.ts",
    "db:studio": "prisma studio",
    "db:reset": "prisma migrate reset && npm run db:seed",
    "lint": "eslint src/**/*.ts",
    "lint:fix": "eslint src/**/*.ts --fix",
    "format": "prettier --write src/**/*.ts",
    "postinstall": "npm run db:generate",
    "docker:build": "docker build -t isra-seeds-backend .",
    "docker:run": "docker run -p 3001:3001 --env-file .env isra-seeds-backend",
    "backup:db": "pg_dump $DATABASE_URL > backup_$(date +%Y%m%d_%H%M%S).sql",
    "restore:db": "psql $DATABASE_URL < backup.sql",
    "logs": "tail -f logs/combined.log",
    "logs:error": "tail -f logs/error.log",
    "health": "curl -f http://localhost:3001/health || exit 1",
    "setup": "npm install && npm run db:generate && npm run db:migrate && npm run db:seed"
  },
  "keywords": [
    "agriculture",
    "seeds",
    "traceability",
    "ISRA",
    "Senegal"
  ],
  "author": "ISRA Saint-Louis",
  "license": "MIT",
  "dependencies": {
    "@prisma/client": "^5.0.0",
    "bcrypt": "^5.1.0",
    "canvas": "^2.11.2",
    "compression": "^1.8.0",
    "cors": "^2.8.5",
    "date-fns": "^4.1.0",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "express-rate-limit": "^7.1.0",
    "helmet": "^7.0.0",
    "isra-seed-traceability-backend": "file:",
    "isra-seeds-backend": "file:",
    "jsonwebtoken": "^9.0.1",
    "jsqr": "^1.4.0",
    "morgan": "^1.10.0",
    "node-cache": "^5.1.2",
    "pdfkit": "^0.17.1",
    "qrcode": "^1.5.3",
    "winston": "^3.17.0",
    "xlsx": "^0.18.5",
    "zod": "^3.25.23"
  },
  "devDependencies": {
    "@types/bcrypt": "^5.0.2",
    "@types/compression": "^1.8.0",
    "@types/cors": "^2.8.18",
    "@types/express": "^4.17.22",
    "@types/jest": "^29.5.14",
    "@types/jsonwebtoken": "^9.0.9",
    "@types/morgan": "^1.9.9",
    "@types/multer": "^1.4.12",
    "@types/node": "^20.17.50",
    "@types/pdfkit": "^0.13.9",
    "@types/qrcode": "^1.5.5",
    "@types/supertest": "^6.0.3",
    "@types/uuid": "^10.0.0",
    "@typescript-eslint/eslint-plugin": "^8.32.1",
    "@typescript-eslint/parser": "^8.32.1",
    "eslint": "^9.27.0",
    "jest": "^29.7.0",
    "nodemon": "^3.1.10",
    "prettier": "^3.5.3",
    "prisma": "^5.22.0",
    "supertest": "^7.1.1",
    "ts-jest": "^29.3.4",
    "ts-node": "^10.9.2",
    "tsx": "^4.19.4",
    "typescript": "^5.8.3"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=8.0.0"
  }
}
